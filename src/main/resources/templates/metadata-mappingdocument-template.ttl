#metadata-mappingdocument-template.ttl

mpv:MappingDocument rdfs:subClassOf dcterms:BibliographicResource .
mpimd:mappingDocument-$mappingDocumentID a mpv:MappingDocument ;
    mpv:mappingDocumentIdentifier "mappingDocument-$mappingDocumentID" ;
    dcterms:identifier "$mappingDocumentID" ;
    mpv:mappingDocumentTitle "$mappingDocumentTitle" ;
    dcterms:title "$mappingDocumentTitle" ;
    mpv:mappingDocumentDateSubmitted "$mappingDocumentDateTimeSubmitted"^^xsd:dateTime;
    dcterms:dateSubmitted "$mappingDocumentDateTimeSubmitted"^^xsd:dateTime;
    mpv:mappingDocumentCreator "$mappingDocumentCreator" ;
    dcterms:creator "$mappingDocumentCreator" ;
    mpv:mappingDocumentSubject "$mappingDocumentSubjects" ;
    dcterms:subject "$mappingDocumentSubjects" ;
    mpv:hasDataset mpids:dataset-$datasetID ;
    mpv:hasDatasetId "$datasetID" ;
    mpv:hasCKANPackageId "$ckanPackageID" ;
    #mpv:hasTriplesMap <TriplesMap1> #generated by mappingpedia-engine
    mpv:hasMappingDocumentFile <$mappingDocumentFilePath> ;
    mpv:hasMappingLanguage "$mappingLanguage" ;
    mpv:hash "$hash" ;
.
